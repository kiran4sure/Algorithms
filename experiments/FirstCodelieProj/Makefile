.PHONY: clean All

All:
	@echo "----------Building project:[ ChatRoom_Mediator - Debug ]----------"
	@cd "ChatRoom_Mediator" && "$(MAKE)" -f  "ChatRoom_Mediator.mk"
clean:
	@echo "----------Cleaning project:[ ChatRoom_Mediator - Debug ]----------"
	@cd "ChatRoom_Mediator" && "$(MAKE)" -f  "ChatRoom_Mediator.mk" clean
